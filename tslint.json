{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-classes-per-file": false,
        "no-namespace": false,
        "object-literal-sort-keys": false,
        "max-line-length": [
            true,
            {
                "limit": 120,
                "ignore-pattern": "^import |^export {(.*?)}| svgPath =|href=|classNames\\(|\\/.*\\/[ig]?;$"
            }
        ],
        "quotemark": [true, "single"]
    },
    "rulesDirectory": []
}
